<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:mnogosearch/pi1/locallang.xml:sheet.basic</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<field_mode>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:mode.label</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items>
									<n0>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:mode.short-form</n0>
										<n1>0</n1>
									</n0>
									<n1>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:mode.long-form</n0>
										<n1>1</n1>
									</n1>
									<n2>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:mode.results</n0>
										<n1>2</n1>
									</n2>
								</items>
								<!--default>2</default-->
							</config>
						</TCEforms>
					</field_mode>
					<field_siteList>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:site_list</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_mnogosearch_indexconfig</allowed>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>100</maxitems>
								<autoSizeMax>7</autoSizeMax>
							</config>
						</TCEforms>
					</field_siteList>
					<field_resultsPage>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:results_page</label>
							<displayCond>FIELD:field_mode:&lt;:2</displayCond>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<autoSizeMax>3</autoSizeMax>
							</config>
						</TCEforms>
					</field_resultsPage>
					<field_options>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>check</type>
								<cols>2</cols>
								<items>
									<n0>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.track</n0> <!-- 0 -->
										<n1></n1>
									</n0>
									<n1>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.cache</n0><!-- 1 -->
										<n1></n1>
									</n1>
									<n2>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.spell</n0><!-- 1 -->
										<n1></n1>
									</n2>
									<n3>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.crosswords</n0><!-- 1 -->
										<n1></n1>
									</n3>
									<n4>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.group</n0><!-- 0 -->
										<n1></n1>
									</n4>
									<n5>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.clones</n0><!-- 1 -->
										<n1></n1>
									</n5>
									<n6>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.phrase</n0><!-- 1 -->
										<n1></n1>
									</n6>
									<n7>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:search_modes.suggest</n0><!-- 1 -->
										<n1></n1>
									</n7>
								</items>
								<default>238</default><!-- 11101110 -->
							</config>
						</TCEforms>
					</field_options>
					<field_resultsPerPage>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:results_per_page</label>
							<displayCond>FIELD:field_mode:&gt;:1</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<eval>required,trim,int,num</eval>
								<range>
									<lower>1</lower>
								</range>
								<default>20</default>
							</config>
						</TCEforms>
					</field_resultsPerPage>
					<field_minWordLen>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:min_word_len</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<eval>required,trim,int,num</eval>
								<range>
									<lower>1</lower>
								</range>
								<default>3</default>
							</config>
						</TCEforms>
					</field_minWordLen>
					<field_maxWordLen>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:max_word_len</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<eval>required,trim,int,num</eval>
								<range>
									<lower>1</lower>
								</range>
								<default>32</default>
							</config>
						</TCEforms>
					</field_maxWordLen>
					<field_excerptSize>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:excerpt_size</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<eval>required,trim,int,num</eval>
								<range>
									<lower>100</lower>
									<upper>4096</upper>
								</range>
								<default>384</default>
							</config>
						</TCEforms>
					</field_excerptSize>
					<field_excerptPadding>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:excerpt_padding</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<eval>required,trim,int,num</eval>
								<range>
									<lower>1</lower>
									<upper>1024</upper>
								</range>
								<default>40</default>
							</config>
						</TCEforms>
					</field_excerptPadding>
					<field_sorting>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:sorting</label>
							<displayCond>FIELD:field_mode:=:2</displayCond>
							<config>
								<type>radio</type>
								<items>
									<n1>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:sorting.RPD</n0>
										<n1>RPD</n1>
									</n1>
									<n2>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:sorting.DRP</n0>
										<n1>DRP</n1>
									</n2>
								</items>
								<default>RPD</default>
							</config>
						</TCEforms>
					</field_sorting>
				</el>
			</ROOT>
		</sDEF>
		<sTmpl>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:mnogosearch/pi1/locallang.xml:sheet.template</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<field_templateMode>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:templatemode.label</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items>
									<n0>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:templatemode.markers</n0>
										<n1>0</n1>
									</n0>
									<n1>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:templatemode.templavoila</n0>
										<n1>1</n1>
									</n1>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</field_templateMode>
					<field_templateObject>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:template_object</label>
							<displayCond>FIELD:field_templateMode:=:1</displayCond>
							<config>
								<type>select</type>
								<foreign_table>tx_templavoila_tmplobj</foreign_table>
								<foreign_table_where>
									AND tx_templavoila_tmplobj.pid=###STORAGE_PID###
									AND tx_templavoila_tmplobj.datastructure LIKE 'EXT:mnogosearch/pi1/ds_%.xml'
									AND tx_templavoila_tmplobj.parent=0
									ORDER BY tx_templavoila_tmplobj.title
								</foreign_table_where>
								<size>1</size>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<autoSizeMax>3</autoSizeMax>
							</config>
						</TCEforms>
					</field_templateObject>
					<field_templateFile>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:template_file</label>
							<displayCond>FIELD:field_templateMode:!=:1</displayCond>
							<config>
								<type>input</type>
								<eval>trim,required</eval>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard&amp;act=file</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										</params>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</field_templateFile>
					<field_templateTestMode>
						<TCEforms>
							<label>LLL:EXT:mnogosearch/pi1/locallang.xml:templatetestmode.label</label>
							<config>
								<type>check</type>
								<items>
									<n0>
										<n0>LLL:EXT:mnogosearch/pi1/locallang.xml:templatetestmode.enable</n0>
										<n1></n1>
									</n0>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</field_templateTestMode>
				</el>
			</ROOT>
		</sTmpl>
	</sheets>
</T3DataStructure>